{
  "name": "camb-dict-api",
  "version": "1.0.0",
  "description": "Cambridge Dictionary API scraper",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "clean": "rimraf dist"
  },
  "keywords": [
